{
    "name": "library-app",
    "version": "2.0.0",
    "description": "",
    "main": "build/main.js",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node build/main",
        "prepare": "node -e \"if(require('fs').existsSync('.git')){process.exit(1)}\" || git config core.hooksPath .githooks",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "lint-staged": "lint-staged"
    },
    "dependencies": {
        "@nestjs/common": "^10.3.7",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.3.7",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.7",
        "@nestjs/serve-static": "^4.0.2",
        "@nestjs/swagger": "^7.3.1",
        "@nestjs/throttler": "^5.1.2",
        "@prisma/client": "^5.12.1",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "dotenv": "^16.4.5",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.4",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^5.0.5",
        "rxjs": "^7.8.1",
        "source-map-support": "^0.5.21",
        "uuid": "^9.0.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IbrohimMirxoshimov/library-app.git"
    },
    "keywords": [
        "library",
        "clean",
        "architecture",
        "solid"
    ],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/IbrohimMirxoshimov/library-app/issues"
    },
    "homepage": "https://github.com/IbrohimMirxoshimov/library-app#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-unused-imports": "^3.1.0",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "prisma": "^5.12.1",
        "typescript": "^5.4.5"
    }
}
